{
	"agent": {
		"always_allow_tool_actions": true,
		"button": true,
		"commit_message_model": {
			"model": "gpt-5-mini",
			"provider": "copilot_chat"
		},
		"default_model": {
			"model": "gpt-5-mini",
			"provider": "copilot_chat"
		},
		"default_profile": "ask",
		"dock": "right",
		"enable_feedback": true,
		"inline_assistant_model": {
			"model": "gpt-4.1",
			"provider": "copilot_chat"
		}
	},
	"audio": {
		"experimental.rodio_audio": true
	},
	"auto_indent": true,
	"autosave": {
		"after_delay": {
			"milliseconds": 1000
		}
	},
	"base_keymap": "SublimeText",
	"buffer_font_family": "Monaspace Neon Var",
	"buffer_font_features": {
		"calt": true,
		"liga": true,
		"ss01": true,
		"ss02": true,
		"ss03": true,
		"ss04": true,
		"ss05": true,
		"ss06": true,
		"ss07": true,
		"ss08": true,
		"ss09": true
	},
	"buffer_font_size": 16,
	"centered_layout": {
		"left_padding": 0.0,
		"right_padding": 0.0
	},
	"code_actions_on_format": {
		"source.fixAll": true,
		"source.organizeImports": true
	},
	"diagnostics": {
		"button": true,
		"include_warnings": true,
		"inline": {
			"enabled": true,
			"max_severity": null,
			"min_column": 0,
			"padding": 4,
			"update_debounce_ms": 150
		},
		"lsp_pull_diagnostics": {
			"debounce_ms": 50,
			"enabled": true
		}
	},
	"disable_ai": false,
	"edit_predictions": {
		"codestral": {},
		"copilot": {},
		"enabled_in_text_threads": false,
		"mode": "eager"
	},
	"ensure_final_newline_on_save": true,
	"file_types": {
		"JSONC": [
			"tsconfig.json",
			"jsconfig.json",
			".vscode/*.json",
			".oxfmtrc.json",
			".oxlintrc.json",
			"*.code-workspace",
			"tsconfig*.json",
			"*.code-snippets"
		],
		"Shell Script": ["zsh", "zshrc", "zprofile", "zshenv", "bashrc", "bash_profile"],
		"TOML": ["toml"],
		"YAML": ["*.yml", "*.yaml", ".github/workflows/*.yml", ".github/workflows/*.yaml"]
	},
	"format_on_save": "on",
	"git": {
		"git_gutter": "tracked_files",
		"hunk_style": null,
		"inline_blame": {
			"delay_ms": 600,
			"enabled": true,
			"show_commit_summary": true
		}
	},
	"hard_tabs": true,
	"icon_theme": "Material Icon Theme",
	"indent_guides": {
		"active_line_width": 1,
		"background_coloring": "disabled",
		"coloring": "indent_aware",
		"enabled": true,
		"line_width": 1
	},
	"inlay_hints": {
		"edit_debounce_ms": 700,
		"enabled": true,
		"scroll_debounce_ms": 50,
		"show_other_hints": true,
		"show_parameter_hints": true,
		"show_type_hints": true
	},
	"languages": {
		"JSON": {
			"code_actions_on_format": {
				"source.fixAll.biome": true,
				"source.organizeImports.biome": true
			},
			"format_on_save": "on",
			"formatter": { "language_server": { "name": "biome" } },
			"hard_tabs": true,
			"language_servers": ["biome", "..."],
			"tab_size": 4
		},
		"JSONC": {
			"code_actions_on_format": {
				"source.fixAll.biome": true,
				"source.organizeImports.biome": true
			},
			"format_on_save": "on",
			"formatter": { "language_server": { "name": "biome" } },
			"hard_tabs": true,
			"language_servers": ["biome", "..."],
			"tab_size": 4
		},

		"Markdown": {
			"format_on_save": "on",
			"formatter": [{ "language_server": { "name": "rumdl" } }],
			"hard_tabs": true,
			"language_servers": ["!marksman", "!markdownlint", "rumdl", "..."],
			"show_whitespaces": "none",
			"soft_wrap": "editor_width",
			"tab_size": 4
		},
		"Shell Script": {
			"format_on_save": "on",
			"formatter": {
				"external": {
					"arguments": ["-i", "4", "-bn", "-ci", "-sr"],
					"command": "shfmt"
				}
			},
			"hard_tabs": true,
			"language_servers": ["bash-language-server", "..."],
			"tab_size": 4
		},

		"TOML": {
			"format_on_save": "on",
			"formatter": "language_server",
			"hard_tabs": true,
			"tab_size": 4
		},
		"TSX": {
			"code_actions_on_format": {
				"source.fixAll.biome": true,
				"source.fixAll.oxc": true,
				"source.organizeImports.biome": true
			},
			"format_on_save": "on",
			"formatter": [
				{
					"external": {
						"arguments": ["x", "--bun", "oxfmt", "--stdin-filepath", "{buffer_path}"],
						"command": "bun"
					}
				},
				{ "language_server": { "name": "biome" } }
			],
			"hard_tabs": true,
			"language_servers": [
				"!tailwindcss-language-server",
				"oxfmt",
				"!typescript-language-server",
				"!eslint",
				"tsgo",
				"vtsls",
				"..."
			],
			"tab_size": 4
		},
		"TypeScript": {
			"code_actions_on_format": {
				"source.fixAll.biome": true,
				"source.fixAll.oxc": true,
				"source.organizeImports.biome": true
			},
			"format_on_save": "on",
			"formatter": [
				{
					"external": {
						"arguments": ["x", "--bun", "oxfmt", "--stdin-filepath", "{buffer_path}"],
						"command": "bun"
					}
				},
				{ "language_server": { "name": "biome" } }
			],
			"hard_tabs": true,
			"language_servers": [
				"!tailwindcss-language-server",
				"!oxfmt",
				"!typescript-language-server",
				"!eslint",
				"tsgo",
				"vtsls",
				"..."
			],
			"tab_size": 4
		},

		"YAML": {
			"format_on_save": "on",
			"hard_tabs": false,
			"tab_size": 2
		}
	},
	"lsp": {
		"bash-language-server": {
			"settings": {
				"bashIde": {
					"globPattern": "**/*@(.sh|.inc|.bash|.command|.zsh)"
				}
			}
		},

		"biome": {
			"binary": {
				"arguments": ["x", "--bun", "biome", "lsp-proxy"],
				"path": "bun"
			},
			"settings": {
				"biome": {
					"configurationPath": "./biome.jsonc",
					"rename": true,
					"requireConfiguration": true
				}
			}
		},

		"eslint": {
			"settings": {
				"codeActionOnSave": {
					"mode": "all"
				},
				"experimental": {
					"useFlatConfig": true
				},
				"format": true,
				"nodePath": null,
				"onIgnoredFiles": "off",
				"options": {
					"flags": ["--import=jiti/register"]
				},
				"packageManager": "bun",
				"quiet": false,
				"run": "onType",
				"runtime": "node",
				"trace.server": "verbose",
				"validate": ["typescript", "typescriptreact", "javascript", "json", "jsonc", "yaml", "markdown"],
				"workingDirectory": {
					"mode": "auto"
				}
			}
		},

		"marksman": {
			"initialization_options": {}
		},
		"oxlint": {
			"initialization_options": {
				"settings": {
					"configPath": "./.oxlintrc.json",
					"disableNestedConfig": false,
					"fixKind": "safe_fix",
					"run": "onType",
					"typeAware": true,
					"unusedDisableDirectives": "deny"
				}
			}
		},
		"snippet-completion-server": {
			"settings": {
				"feature_paths": true
			}
		},

		"taplo": {
			"initialization_options": {
				"configuration": {
					"formatting": {
						"align_comments": true,
						"align_entries": false,
						"allowed_blank_lines": 1,
						"array_auto_expand": true,
						"array_trailing_comma": true,
						"column_width": 120,
						"compact_arrays": true,
						"compact_inline_tables": false,
						"indent_entries": false,
						"indent_tables": false,
						"trailing_newline": true
					}
				}
			}
		},
		"vtsls": {
			"enable_lsp_tasks": true,
			"initialization_options": {
				"javascript": {
					"inlayHints": {
						"enumMemberValues": {
							"enabled": true
						},
						"functionLikeReturnTypes": {
							"enabled": true
						},
						"parameterNames": {
							"enabled": "all",
							"suppressWhenArgumentMatchesName": false
						},
						"parameterTypes": {
							"enabled": true
						},
						"propertyDeclarationTypes": {
							"enabled": true
						},
						"variableTypes": {
							"enabled": false
						}
					}
				},
				"tsserver": {
					"maxTsServerMemory": 8192
				},
				"typescript": {
					"inlayHints": {
						"enumMemberValues": {
							"enabled": true
						},
						"functionLikeReturnTypes": {
							"enabled": true
						},
						"parameterNames": {
							"enabled": "all",
							"suppressWhenArgumentMatchesName": false
						},
						"parameterTypes": {
							"enabled": true
						},
						"propertyDeclarationTypes": {
							"enabled": true
						},
						"variableTypes": {
							"enabled": false
						}
					},
					"preferences": {
						"includePackageJsonAutoImports": "on"
					},
					"suggest": {
						"autoImports": true
					},
					"updateImportsOnFileMove": "always"
				}
			},
			"settings": {
				"javascript": {
					"inlayHints": {
						"enumMemberValues": {
							"enabled": true
						},
						"functionLikeReturnTypes": {
							"enabled": true
						},
						"parameterNames": {
							"enabled": "all",
							"suppressWhenArgumentMatchesName": false
						},
						"parameterTypes": {
							"enabled": true
						},
						"propertyDeclarationTypes": {
							"enabled": true
						},
						"variableTypes": {
							"enabled": false
						}
					},
					"suggest": {
						"autoImports": true
					},
					"tsserver": {
						"maxTsServerMemory": 8192
					},
					"updateImportsOnFileMove": "always"
				},
				"typescript": {
					"inlayHints": {
						"enumMemberValues": {
							"enabled": true
						},
						"functionLikeReturnTypes": {
							"enabled": true
						},
						"parameterNames": {
							"enabled": "all",
							"suppressWhenArgumentMatchesName": false
						},
						"parameterTypes": {
							"enabled": true
						},
						"propertyDeclarationTypes": {
							"enabled": true
						},
						"variableTypes": {
							"enabled": false
						}
					},
					"suggest": {
						"autoImports": true
					},
					"tsserver": {
						"maxTsServerMemory": 8192
					},
					"updateImportsOnFileMove": "always"
				}
			}
		}
	},
	"project_panel": {
		"button": true,
		"default_width": 240,
		"dock": "left",
		"file_icons": true,
		"folder_icons": true,
		"git_status": true,
		"indent_size": 20
	},
	"redact_private_values": false,
	"remove_trailing_whitespace_on_save": true,
	"scrollbar": {
		"cursors": true,
		"diagnostics": "all",
		"git_diff": true,
		"search_results": true,
		"selected_symbol": true,
		"show": "auto"
	},
	"show_completions_on_input": true,
	"show_edit_predictions": true,
	"tab_size": 4,
	"tabs": {
		"close_position": "right",
		"file_icons": true,
		"git_status": true
	},
	"terminal": {
		"button": true,
		"default_height": 320,
		"default_width": 640,
		"dock": "bottom",
		"font_family": "Monaspace Neon",
		"font_features": {
			"calt": true,
			"liga": true,
			"ss01": true,
			"ss02": true,
			"ss03": true,
			"ss04": true,
			"ss05": true,
			"ss06": true,
			"ss07": true,
			"ss08": true,
			"ss09": true
		},
		"max_scroll_history_lines": 100000,
		"toolbar": {
			"breadcrumbs": true
		},
		"working_directory": "current_project_directory"
	},
	"theme": {
		"dark": "Zedokai (Filter Spectrum)",
		"light": "One Light",
		"mode": "system"
	},
	"toolbar": {
		"breadcrumbs": true,
		"quick_actions": true
	},
	"ui_font_family": "SF Pro",

	"ui_font_features": {
		"calt": true
	},
	"ui_font_size": 16,
	"use_on_type_format": true,
	"vim_mode": false
}
