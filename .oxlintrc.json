{
	"$schema": "./node_modules/oxlint/configuration_schema.json",
	"categories": {
		"correctness": "error",
		"nursery": "error",
		"pedantic": "error",
		"perf": "error",
		"restriction": "error",
		// "style": "error",
		"suspicious": "error",
	},
	"env": {
		"builtin": true,
		"es2022": true,
		"node": true,
	},
	"extends": [],
	"globals": {
		"Bun": "readonly",
	},
	"ignorePatterns": ["dist", "test-circular-deps/**", "do-not-sync-ever/**", "**/*.js"],
	"overrides": [
		{
			"files": ["**/__tests__/*.test.ts"],
			"globals": {
				"global": "writeable",
			},
			"plugins": ["eslint", "jest", "typescript", "regex", "node"],
			"rules": {
				"max-lines": "off",
				"max-lines-per-function": "off",
				"no-console": "off",
				"no-non-null-assertion": "off",
			},
		},
		{
			"files": ["src/rules/enforce-ianitor-check-type.ts", "src/rules/require-react-component-keys.ts", "src/index.ts"],
			"rules": {
				"@typescript-eslint/no-unsafe-argument": "off",
				"@typescript-eslint/no-unsafe-assignment": "off",
				"@typescript-eslint/no-unsafe-enum-comparison": "off",
				"@typescript-eslint/no-unsafe-member-access": "off",
				"@typescript-eslint/no-unsafe-type-assertion": "off",
				"@typescript-eslint/switch-exhaustiveness-check": "off",
			},
		},
	],
	"plugins": ["eslint", "import", "jsdoc", "node", "oxc", "promise", "regex", "typescript", "unicorn"],
	"rules": {
		"@typescript-eslint/ban-ts-comment": "error",
		"@typescript-eslint/no-duplicate-enum-values": "error",
		"@typescript-eslint/no-empty-object-type": "error",
		"@typescript-eslint/no-explicit-any": "error",
		"@typescript-eslint/no-extra-non-null-assertion": "error",
		"@typescript-eslint/no-misused-new": "error",
		"@typescript-eslint/no-namespace": "error",
		"@typescript-eslint/no-non-null-asserted-optional-chain": "error",
		"@typescript-eslint/no-require-imports": "error",
		"@typescript-eslint/no-this-alias": "error",
		"@typescript-eslint/no-unnecessary-type-constraint": "error",
		"@typescript-eslint/no-unsafe-declaration-merging": "error",
		"@typescript-eslint/no-unsafe-function-type": "error",
		"@typescript-eslint/no-wrapper-object-types": "error",
		"@typescript-eslint/prefer-as-const": "error",
		"@typescript-eslint/prefer-namespace-keyword": "error",
		"@typescript-eslint/triple-slash-reference": "error",
		"array-type": "off",
		"curly": "off",
		"eslint/max-lines-per-function": [
			"error",
			{
				"IIFEs": false,
				"max": 120,
				"skipBlankLines": false,
				"skipComments": false,
			},
		],
		// WHY THE FUCK DOES THIS SHIT CONFLICT WITH ANOTHER RULE FUCK OFF
		"eslint/require-await": "off",
		"for-direction": "error",
		"func-style": ["error", "declaration"],
		"import/max-dependencies": "off",
		"import/no-default-export": "off",
		"import/no-named-as-default": "off",
		"jsdoc/require-param-type": "off",
		"jsdoc/require-property-type": "off",
		"jsdoc/require-returns-type": "off",
		"max-lines": "off",
		"max-lines-per-function": "off",
		"no-array-constructor": "off",
		"no-async-promise-executor": "error",
		"no-bitwise": "off",
		"no-case-declarations": "error",
		"no-class-assign": "off",
		// "no-class-assign": "error",
		"no-compare-neg-zero": "error",
		"no-cond-assign": "error",
		"no-console": "off",
		"no-const-assign": "off",
		// "no-const-assign": "error",
		"no-constant-binary-expression": "error",
		"no-constant-condition": "error",
		"no-continue": "off",
		"no-control-regex": "error",
		"no-debugger": "error",
		"no-delete-var": "error",
		"no-dupe-class-members": "off",
		// "no-dupe-class-members": "error",
		"no-dupe-else-if": "error",
		"no-dupe-keys": "off",
		// "no-dupe-keys": "error",
		"no-duplicate-case": "error",
		"no-empty": "error",
		"no-empty-character-class": "error",
		"no-empty-pattern": "error",
		"no-empty-static-block": "error",
		"no-ex-assign": "error",
		"no-extra-boolean-cast": "error",
		"no-fallthrough": "error",
		"no-func-assign": "off",
		// "no-func-assign": "error",
		"no-global-assign": "error",
		"no-import-assign": "off",
		// "no-import-assign": "error",
		"no-invalid-regexp": "error",
		"no-irregular-whitespace": "error",
		"no-loss-of-precision": "error",
		"no-magic-numbers": "off",
		"no-new-array": "off",
		"no-new-native-nonconstructor": "off",
		"no-non-null-assertion": "off",
		// "no-new-native-nonconstructor": "error",
		"no-nonoctal-decimal-escape": "error",
		"no-obj-calls": "off",
		"no-optional-chaining": "off",
		// "no-obj-calls": "error",
		"no-plusplus": "off",
		"no-prototype-builtins": "error",
		"no-redeclare": "off",
		// "no-redeclare": "error",
		"no-regex-spaces": "error",
		"no-self-assign": "error",
		"no-setter-return": "off",
		// "no-setter-return": "error",
		"no-shadow-restricted-names": "error",
		"no-sparse-arrays": "error",
		"no-ternary": "off",
		"no-this-before-super": "off",
		// "no-this-before-super": "error",
		"no-undefined": "off",
		"no-unexpected-multiline": "error",
		"no-unsafe-finally": "error",
		"no-unsafe-negation": "off",
		// "no-unsafe-negation": "error",
		"no-unsafe-optional-chaining": "error",
		"no-unused-expressions": "error",
		"no-unused-labels": "error",
		"no-unused-private-class-members": "error",
		"no-unused-vars": "error",
		"no-useless-backreference": "error",
		"no-useless-catch": "error",
		"no-useless-escape": "error",
		"no-var": "error",
		"no-with": "off",
		"oxc/no-async-await": "off",
		"oxc/no-const-enum": "off",
		"oxc/no-rest-spread-properties": "off",
		"prefer-rest-params": "error",
		"prefer-spread": "error",
		// "no-with": "error",
		"require-yield": "error",
		"typescript/array-type": "allow",
		"use-isnan": "error",
		"valid-typeof": "error",
	},
	"settings": {
		"jsdoc": {
			"augmentsExtendsReplacesDocs": false,
			"exemptDestructuredRootsFromChecks": false,
			"ignoreInternal": false,
			"ignorePrivate": false,
			"ignoreReplacesDocs": true,
			"implementsReplacesDocs": false,
			"overrideReplacesDocs": true,
			"tagNamePreference": {},
		},
		"jsx-a11y": {
			"attributes": {},
			"components": {},
			"polymorphicPropName": null,
		},
		"next": {
			"rootDir": [],
		},
		"react": {
			"formComponents": [],
			"linkComponents": [],
		},
	},
}
