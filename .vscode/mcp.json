{
	"inputs": [
		{
			"description": "The personal access token for the GitHub MCP.",
			"id": "github-token",
			"password": true,
			"type": "promptString",
		},
	],
	"servers": {
		"codex-subagent": {
			"args": ["codex-as-mcp@latest"],
			"command": "uvx",
			"type": "stdio",
		},
		"context7": {
			"args": ["x", "--bun", "@upstash/context7-mcp"],
			"command": "bun",
			"env": {},
			"type": "stdio",
		},
		"gh-grep": {
			"type": "http",
			"url": "https://mcp.grep.app",
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
		},
		"npm-registry": {
			"args": ["run", "github.com/howmanysmall/npm-registry-mcp/src@main"],
			"command": "go",
			"type": "stdio",
		},
		"package-registry": {
			"args": ["x", "--bun", "package-registry-mcp"],
			"command": "bun",
		},
		"uniprof": {
			"args": ["x", "uniprof", "mcp", "run"],
			"command": "bun",
		},
	},
}
