{
  "name": "@pobammer-ts/eslint-cease-nonsense-rules",
  "version": "0.0.0",
  "description": "A bunch of lints to prevent idiot mistakes I encounter with frequency.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.18",
    "bun": ">=1.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/howmanysmall/eslint-cease-nonsense-rules.git"
  },
  "author": "HowManySmall",
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.11",
    "eslint": "^9.9.0",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "bunx --bun tsc -p tsconfig.json",
    "typecheck": "bunx --bun tsc --noEmit",
    "dev": "bunx --bun tsc -w",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  }
}
