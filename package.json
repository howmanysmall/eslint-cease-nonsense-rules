{
	"author": "HowManySmall",
	"dependencies": {
		"type-fest": "^5.2.0",
		"typebox": "^1.0.51"
	},
	"description": "A bunch of lints to prevent idiot mistakes I encounter with frequency.",
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@mitata/counters": "^0.0.8",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/bun": "latest",
		"@types/node": "^20.14.11",
		"@typescript-eslint/parser": "^8.0.0",
		"@typescript-eslint/types": "^8.0.0",
		"@typescript-eslint/utils": "^8.0.0",
		"@typescript/native-preview": "latest",
		"arktype": "^2.1.25",
		"bumpp": "^10.3.1",
		"eslint": "9.35.0",
		"jiti": "^2.4.2",
		"lint-staged": "^16.2.6",
		"mitata": "^1.0.34",
		"oxfmt": "^0.11.0",
		"oxlint": "^1.25.0",
		"oxlint-tsgolint": "^0.5.1",
		"simple-git-hooks": "^2.13.1",
		"sury": "^11.0.0-alpha.4",
		"typescript": "^5.6.3"
	},
	"engines": {
		"bun": ">=1.1.0",
		"node": ">=18.18"
	},
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"license": "MIT",
	"lint-staged": {
		"*": "bun run oxc --fix"
	},
	"main": "./dist/index.js",
	"name": "@pobammer-ts/eslint-cease-nonsense-rules",
	"peerDependencies": {
		"@typescript-eslint/parser": "^8.0.0",
		"@typescript-eslint/utils": "^8.0.0",
		"eslint": "^9.35.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/howmanysmall/eslint-cease-nonsense-rules.git"
	},
	"scripts": {
		"biome:ci": "bun x --bun biome ci",
		"build": "bun x --bun tsgo -p tsconfig.json",
		"build:compatible": "bun x --bun tsc -p tsconfig.json",
		"dev": "bun x --bun tsgo -w",
		"format": "bun x oxfmt",
		"lint": "bun run oxc . && bun x --bun biome lint .",
		"oxc": "bun x --bun oxlint --type-aware",
		"prepare": "bun x --bun simple-git-hooks",
		"prepublishOnly": "bun run build",
		"release": "bumpp",
		"typecheck": "bun x --bun tsgo --noEmit"
	},
	"sideEffects": false,
	"simple-git-hooks": {
		"pre-commit": "bun run lint-staged"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "1.0.1"
}
