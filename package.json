		"just-diff": "^6.0.2",
		"knip": "^5.82.1",
		"lint-staged": "^16.2.7",
		"mitata": "^1.0.34",
		"oxfmt": "^0.27.0",
		"oxlint": "^1.42.0",
		"oxlint-tsgolint": "^0.11.4",
		"picocolors": "^1.1.1",
		"pretty-bytes": "^7.1.0",
		"pretty-ms": "^9.3.0",
		"rotating-file-stream": "^3.2.7",
		"simple-git-hooks": "^2.13.1",
		"source-map": "^0.7.6",
		"temporal-polyfill": "^0.3.0",
		"ts-api-utils": "^2.4.0",
		"ts-case-convert": "^2.1.0",
		"type-fest": "^5.4.2",
		"typescript": "^5.6.3",
		"vue-eslint-parser": "^10.2.0"
	},
	"engines": {
		"bun": ">=1.1.0",
		"node": ">=20.18"
	},
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"license": "MIT",
	"lint-staged": {
		"*": ["bun run lint:fix", "bun x --bun biome check . --fix"]
	},
	"main": "./dist/index.js",
	"name": "@pobammer-ts/eslint-cease-nonsense-rules",
	"packageManager": "bun@1.3.8",
	"peerDependencies": {
		"@typescript-eslint/parser": "^8.54.0",
		"@typescript-eslint/utils": "^8.54.0",
		"eslint": "^9.39.2",
		"oxfmt": ">0.19.0"
	},
	"peerDependenciesMeta": {
		"oxfmt": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/howmanysmall/eslint-cease-nonsense-rules.git"
	},
	"scripts": {
		"add-tsgolint": "bun add -D oxlint-tsgolint@latest",
		"biome:check": "biome check",
		"biome:ci": "biome ci",
		"biome:lint": "biome lint",
		"build": "./scripts/build.ts",
		"build:legacy": "tsgo -p tsconfig.json",
		"commitlint": "commitlint --edit",
		"dev": "tsgo -w",
		"docs:build": "bun --cwd documentation build",
		"docs:dev": "bun --cwd documentation dev",
		"format": "oxfmt",
		"knip": "knip-bun",
		"lint": "bun run oxc . && bun run biome:check src tests",
		"lint:fix": "bun run oxc . --fix && bun run biome:check . --fix",
		"oxc": "oxlint --type-aware",
		"oxc:complete": "oxlint --type-aware --type-aware",
		"oxc:raw": "oxlint",
		"prepare": "simple-git-hooks",
		"prepublishOnly": "bun run build",
		"release": "bumpp",
		"test": "bun test --randomize",
		"test:watch": "bun run test --watch",
		"type-check": "tsgo --noEmit",
		"type-check:safe": "tsc --noEmit"
	},
	"sideEffects": false,
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "1.20.2"
}
