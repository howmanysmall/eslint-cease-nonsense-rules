	},
	"description": "A bunch of lints to prevent idiot mistakes I encounter with frequency.",
	"devDependencies": {
		"@babel/core": "^7.28.6",
		"@babel/eslint-parser": "^7.28.6",
		"@biomejs/biome": "^2.3.13",
		"@commitlint/config-conventional": "^20.4.0",
		"@commitlint/types": "^20.4.0",
		"@jsr/cliffy__command": "^1.0.0-rc.8",
		"@mitata/counters": "^0.0.8",
		"@octokit/rest": "^22.0.1",
		"@opencode-ai/plugin": "^1.1.47",
		"@rbxts/types": "^1.0.905",
		"@toon-format/toon": "^2.1.0",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/bun": "^1.3.8",
		"@types/estree": "^1.0.8",
		"@types/node": "^22.0.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@typescript-eslint/rule-tester": "^8.54.0",
		"@typescript-eslint/scope-manager": "^8.54.0",
		"@typescript-eslint/types": "^8.54.0",
		"@typescript-eslint/utils": "^8.54.0",
		"@typescript/native-preview": "7.0.0-dev.20260130.1",
		"arkenv": "^0.9.2",
		"arktype": "^2.1.29",
		"better-result": "^2.6.0",
		"bumpp": "^10.4.0",
